<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:andriod="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    andriod:layout_width="match_parent"
    andriod:layout_height="match_parent"
    andriod:padding="16dp"
    tools:context=".TitleFragment" >

    <fragment
        andriod:id="@+id/nav_host_fragment"
        andriod:name="androidx.navigation.fragment.NavHostFragment"
        app:defaultNavHost="true"
        app:navGraph="@navigation/nav_graph"
        andriod:layout_width="match_parent"
        andriod:layout_height="match_parent" />
</FrameLayout>
